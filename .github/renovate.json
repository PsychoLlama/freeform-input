{
  "extends": [
    "config:base"
  ],
  "ignorePaths": [
    "./example"
  ],
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch",
        "digest"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    }
  ]
}
